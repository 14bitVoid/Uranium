{
    "platform": "ultiscantastic.obj",
    "required_plugins": 
    [
        "Console Logger", 
        "ScannerBackend"
    ],
    "machine_settings": 
    [
        {
            "key":"machine_width",
            "default": 100
        },
        {
            "key":"machine_height",
            "default": 100
        }
    ],
    "Categories": [
        {
            "key": "calibration",
            "label": "Calibration",
            "visible": true,
            "Settings": [
                {
                    "key": "calibration_type",
                    "label": "Calibration type",
                    "type": "enum",
                    "default": "Board",
                    "options": [
                        "Board",
                        "Corner"
                    ]
                },
                {
                    "key": "setup_type",
                    "label": "Setup",
                    "type": "enum",
                    "default": "Platform",
                    "options": [
                        "Platform",
                        "Freeform"
                    ]
                },
                {
                    "key": "number_of_calibration_boards",
                    "label": "num calib boards",
                    "type": "int",
                    "default": 1,
                    "min_value": 0,
                    "active_if": {
                        "setting": "calibration_type",
                        "value": "Board"
                    },
                    "max_value": 250,
                    "children": [
                        {
                            "key": "number_of_calibration_boards_scanner",
                            "label": "num calib boards scanner",
                            "type": "int",
                            "default": 1,
                            "min_value": 0,
                            "max_value": 250
                        },
                        {
                            "key": "number_of_calibration_boards_projector",
                            "label": "num calib boards projector",
                            "type": "int",
                            "default": 1,
                            "active_if": {
                                "setting": "setup_type",
                                "value": "Platform"
                            },
                            "min_value": 0,
                            "max_value": 250
                        }
                    ]
                }
            ]
        },
        {
            "key": "meshing",
            "label": "Meshing",
            "visible": true,
            "Settings": [
                {
                    "key": "TestSetting",
                    "label": "testtest",
                    "type": "int",
                    "default": 210,
                    "min_value": 150,
                    "max_value": 280
                }
            ]
        },
        {
            "key": "canning",
            "label": "Scanning",
            "visible": true,
            "Settings": [
                {
                    "key": "pattern_type",
                    "label": "Pattern type",
                    "type": "enum",
                    "default": "Grey code",
                    "options": [
                        "Grey code",
                        "Phase shift"
                    ]
                },
                {
                    "key": "num_phase_pattern_shifts",
                    "label": "Pattern shifts",
                    "type": "int",
                    "default": 16,
                    "min_value": 3,
                    "max_value": 150,
                    "active_if": {
                        "setting": "pattern_type",
                        "value": "Phase shift"
                    }
                }
            ]
        }
    ]
}